{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=.\\sqlexpress;Initial Catalog=Ginder;Integrated Security=true; Connection Timeout=300000"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AddMigration": "dotnet ef migrations add AddUserMigration --project ./src/Ginder.Infra --startup-project ./src/Ginder.API",
  "UpdateDatabase": "dotnet ef database update --project ./src/Ginder.Infra --startup-project ./src/Ginder.API"
}
